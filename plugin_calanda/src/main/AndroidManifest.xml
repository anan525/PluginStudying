<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.voyah.plugin_calanda">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.PluginStudy">
        <service android:name=".CalandaService"></service>

        <activity android:name=".TopActivity" />
        <activity android:name=".SecondActivity" />

        <receiver android:name=".CalandaReceiver">
            <intent-filter>
                <action android:name="com.voyah.plugin_calanda.Static"></action>
            </intent-filter>
        </receiver>
    </application>

</manifest>